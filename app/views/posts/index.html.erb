<div class="container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <div class="post-wrapper">
        <div class="post clearfix">
        <% @posts.each do |post| %>
            <div class="post-details">
              <span class="post-title"><%= link_to post.title, post %></span>
              <br>
              <div class="">
                Publicado por <%= post.user.email %>  hace
                <br>
                   Fecha creaci√≥n <%= post.created_at %>  Fecha actual <%=  Time.now %>
              </div>
              <span class="post-body"><%= post.body %></span>
            </div>
        <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
